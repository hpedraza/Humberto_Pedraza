@{ Layout = null;}
<div class="modal-dialog" style="margin-top: 15px;">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">
                <span>&times;</span>
            </button>
            <div class="resumeHeading">
                <h2>Ant Colony Slide Show</h2>
            </div>
        </div>
        <div class="modal-body">
            <div class="container portfolioSlideShowContainer">
                <div id="slideShow7" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                        <li data-target="#slideShow7" data-slide-to="0" class="active"></li>
                        <li data-target="#slideShow7" data-slide-to="1"></li>
                        <li data-target="#slideShow7" data-slide-to="2"></li>
                        <li data-target="#slideShow7" data-slide-to="3"></li>
                        <li data-target="#slideShow7" data-slide-to="4"></li>
                    </ol>
                    <div class="carousel-inner" role="listbox">
                        <div class="item active portfolioImage" align="center">
                            <img src="~/PortfolioFiles/AntColony_Humberto_Pedraza/Captures/1.PNG" />
                            <div class="carousel-caption">1</div>
                        </div>
                        <div class="item portfolioImage" align="center">
                            <img src="~/PortfolioFiles/AntColony_Humberto_Pedraza/Captures/2.PNG" />
                            <div class="carousel-caption">2</div>
                        </div>
                        <div class="item portfolioImage" align="center">
                            <img src="~/PortfolioFiles/AntColony_Humberto_Pedraza/Captures/3.PNG" />
                            <div class="carousel-caption">3</div>
                        </div>
                        <div class="item portfolioImage" align="center">
                            <img src="~/PortfolioFiles/AntColony_Humberto_Pedraza/Captures/path taken by ants.PNG" />
                            <div class="carousel-caption">4</div>
                        </div>
                        <div class="item portfolioImage" align="center">
                            <img src="~/PortfolioFiles/AntColony_Humberto_Pedraza/Captures/graph.PNG" />
                            <div class="carousel-caption">5</div>
                        </div>
                    </div>
                    <a class="left carousel-control" href="#slideShow7" data-slide="prev">
                        <span class="glyphicon glyphicon-chevron-left"></span>
                    </a>
                    <a class="right carousel-control" href="#slideShow7" data-slide="next">
                        <span class="glyphicon glyphicon-chevron-right"></span>
                    </a>
                </div>
            </div>
    </div><!-- END OF MODAL BODY -->

    <div class="modal-footer">
        <!-- Add download resume option -->
        <button type="button" class="btn btn-warning" data-dismiss="modal">Close</button>
    </div>
    </div>
</div>


