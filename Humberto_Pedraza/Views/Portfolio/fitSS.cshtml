@{ Layout = null;}
<div class="modal-dialog" style="margin-top: 15px;">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">
                <span>&times;</span>
            </button>
            <div class="resumeHeading">
                <h2>Get Fit Web App Slide Show</h2>
            </div>
        </div>
        <div class="modal-body">
            <div class="container portfolioSlideShowContainer">
                <div id="getfitSlideShow" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                        <li data-target="#getfitSlideShow" data-slide-to="0" class="active"></li>
                        <li data-target="#getfitSlideShow" data-slide-to="1"></li>
                        <li data-target="#getfitSlideShow" data-slide-to="2"></li>
                        <li data-target="#getfitSlideShow" data-slide-to="3"></li>
                        <li data-target="#getfitSlideShow" data-slide-to="4"></li>
                        <li data-target="#getfitSlideShow" data-slide-to="5"></li>
                        <li data-target="#getfitSlideShow" data-slide-to="6"></li>
                        <li data-target="#getfitSlideShow" data-slide-to="7"></li>
                        <li data-target="#getfitSlideShow" data-slide-to="8"></li>
                        <li data-target="#getfitSlideShow" data-slide-to="9"></li>
                        <li data-target="#getfitSlideShow" data-slide-to="10"></li>
                    </ol>
                    <div class="carousel-inner" role="listbox">
                        <div class="item active portfolioImage" align="center">
                            <img src="~/PortfolioFiles/getfit_imgs/getfit_home_1.PNG" />
                            <div class="carousel-caption">1</div>
                        </div>
                        <div class="item portfolioImage" align="center">
                            <img src="~/PortfolioFiles/getfit_imgs/getfit_home_2.PNG" />
                            <div class="carousel-caption">2</div>
                        </div>
                        <div class="item portfolioImage" align="center">
                            <img src="~/PortfolioFiles/getfit_imgs/getfit_signin.PNG" />
                            <div class="carousel-caption">3</div>
                        </div>
                        <div class="item portfolioImage" align="center">
                            <img src="~/PortfolioFiles/getfit_imgs/user_login_page.PNG" />
                            <div class="carousel-caption">4</div>
                        </div>
                        <div class="item portfolioImage" align="center">
                            <img src="~/PortfolioFiles/getfit_imgs/getfit_log_1.PNG" />
                            <div class="carousel-caption">5</div>
                        </div>
                        <div class="item portfolioImage" align="center">
                            <img src="~/PortfolioFiles/getfit_imgs/getfit_log_2.PNG" />
                            <div class="carousel-caption">6</div>
                        </div>
                        <div class="item portfolioImage" align="center">
                            <img src="~/PortfolioFiles/getfit_imgs/getfit_log_3.PNG" />
                            <div class="carousel-caption">7</div>
                        </div>
                        <div class="item portfolioImage" align="center">
                            <img src="~/PortfolioFiles/getfit_imgs/getfit_log_4.PNG" />
                            <div class="carousel-caption">8</div>
                        </div>

                    </div>
                    <a class="left carousel-control" href="#getfitSlideShow" data-slide="prev">
                        <span class="glyphicon glyphicon-chevron-left"></span>
                    </a>
                    <a class="right carousel-control" href="#getfitSlideShow" data-slide="next">
                        <span class="glyphicon glyphicon-chevron-right"></span>
                    </a>
                </div>
            </div>
        </div>
    </div><!-- END OF MODAL BODY -->
    <div>

    </div>
    <div class="modal-footer">
        <!-- Add download resume option -->
        <button type="button" class="btn btn-warning" data-dismiss="modal">Close</button>
    </div>
</div>


